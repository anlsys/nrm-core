{
    "extraStaticPassiveSensors": [
        {
            "mapKey": "example extra static passive power sensor",
            "mapValue": {
                "range": {
                    "_2": 40,
                    "_1": 1
                },
                "sensorArguments": [
                    "30"
                ],
                "tags": [
                    "Power"
                ],
                "sensorBinary": "echo"
            }
        }
    ],
    "pmpi_lib": "pmpi_lib",
    "verbose": "Error",
    "logfile": "/tmp/nrm.log",
    "singularity": false,
    "argo_nodeos_config": "argo_nodeos_config",
    "controlCfg": {
        "fixedPower": {
            "fromuW": 250000000
        }
    },
    "upstreamCfg": {
        "upstreamBindAddress": "*",
        "rpcPort": 3456,
        "pubPort": 2345
    },
    "libnrmPath": null,
    "activeSensorFrequency": {
        "fromHz": 1
    },
    "perf": "perf",
    "argo_perf_wrapper": "nrm-perfwrapper",
    "downstreamCfg": {
        "downstreamBindAddress": "ipc:///tmp/nrm-downstream-event"
    },
    "nodeos": false,
    "hwloc": "hwloc",
    "extraStaticActuators": [],
    "raplCfg": {
        "referencePower": {
            "fromuW": 250000000
        },
        "raplActions": [
            {
                "fromuW": 100000000
            },
            {
                "fromuW": 200000000
            }
        ],
        "raplPath": "/sys/devices/virtual/powercap/intel-rapl"
    },
    "dummy": true,
    "slice_runtime": "Dummy",
    "hwmonCfg": {
        "hwmonPath": "/sys/class/hwmon",
        "hwmonEnabled": true
    }
}