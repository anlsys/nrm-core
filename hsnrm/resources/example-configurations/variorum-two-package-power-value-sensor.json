{
    "extraStaticPassiveSensors": [
        {
            "mapKey": "Sensor that gets package power limits for package 0 through variorum",
            "mapValue": {
                "range": {
                    "_2": 40,
                    "_1": 1
                },
                "sensorBehavior": "Cumulative",
                "sensorArguments": [
                    "-c",
                    "variorum-print-power-limits-example | awk '{ if ($1 == \"_PACKAGE_ENERGY_STATUS\" && $2 == \"0x610\" && $4 == 0 ) { print $6 } }'"
                ],
                "tags": [
                    "Power"
                ],
                "sensorBinary": "bash"
            }
        },
        {
            "mapKey": "Sensor that gets package power limits for package 1 through variorum",
            "mapValue": {
                "range": {
                    "_2": 40,
                    "_1": 1
                },
                "sensorBehavior": "Cumulative",
                "sensorArguments": [
                    "-c",
                    "variorum-print-power-limits-example | awk '{ if ($1 == \"_PACKAGE_ENERGY_STATUS\" && $2 == \"0x610\" && $4 == 1 ) { print $6 } }'"
                ],
                "tags": [
                    "Power"
                ],
                "sensorBinary": "bash"
            }
        }
    ],
    "pmpi_lib": "pmpi_lib",
    "verbose": "Error",
    "logfile": "/tmp/nrm.log",
    "singularity": false,
    "argo_nodeos_config": "argo_nodeos_config",
    "controlCfg": "NoControl",
    "upstreamCfg": {
        "upstreamBindAddress": "*",
        "rpcPort": 3456,
        "pubPort": 2345
    },
    "libnrmPath": null,
    "perf": "perf",
    "argo_perf_wrapper": "nrm-perfwrapper",
    "downstreamCfg": {
        "downstreamBindAddress": "ipc:///tmp/nrm-downstream-event"
    },
    "nodeos": false,
    "hwloc": "hwloc",
    "extraStaticActuators": [],
    "raplCfg": {
        "referencePower": {
            "fromuW": 250000000
        },
        "raplActions": [
            {
                "fromuW": 100000000
            },
            {
                "fromuW": 200000000
            }
        ],
        "raplPath": "/sys/devices/virtual/powercap/intel-rapl"
    },
    "dummy": true,
    "slice_runtime": "Dummy",
    "hwmonCfg": {
        "hwmonPath": "/sys/class/hwmon",
        "hwmonEnabled": true
    },
    "passiveSensorFrequency": {
        "fromHz": 1
    }
}