{
    "extraStaticPassiveSensors": [],
    "pmpi_lib": "pmpi_lib",
    "verbose": "Error",
    "logfile": "/tmp/nrm.log",
    "singularity": false,
    "argo_nodeos_config": "argo_nodeos_config",
    "controlCfg": "NoControl",
    "upstreamCfg": {
        "upstreamBindAddress": "*",
        "rpcPort": 3456,
        "pubPort": 2345
    },
    "libnrmPath": null,
    "perf": "perf",
    "argo_perf_wrapper": "nrm-perfwrapper",
    "downstreamCfg": {
        "downstreamBindAddress": "ipc:///tmp/nrm-downstream-event"
    },
    "nodeos": false,
    "hwloc": "hwloc",
    "extraStaticActuators": [
        {
            "mapKey": "example extra actuator",
            "mapValue": {
                "actuatorBinary": "bash",
                "actions": [
                    1,
                    2
                ],
                "actuatorArguments": [
                    "-c",
                    "echo $@ >> /tmp/test-nrm-example-extra-actuator",
                    "-o"
                ],
                "referenceAction": 1
            }
        }
    ],
    "raplCfg": {
        "referencePower": {
            "fromuW": 250000000
        },
        "raplActions": [
            {
                "fromuW": 100000000
            },
            {
                "fromuW": 200000000
            }
        ],
        "raplPath": "/sys/devices/virtual/powercap/intel-rapl"
    },
    "dummy": true,
    "slice_runtime": "Dummy",
    "hwmonCfg": {
        "hwmonPath": "/sys/class/hwmon",
        "hwmonEnabled": true
    },
    "passiveSensorFrequency": {
        "fromHz": 1
    }
}